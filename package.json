{
  "name": "laravel-vue-forms",
  "version": "1.0.0",
  "description": "Laravel Vue Forms & Grid",
  "license": "MIT",
  "main": "dist/laravel-vue-forms.ssr.js",
  "browser": "dist/laravel-vue-forms.esm.js",
  "module": "dist/laravel-vue-forms.esm.js",
  "unpkg": "dist/laravel-vue-forms.min.js",
  "types": "laravel-vue-forms.d.ts",
  "files": [
    "dist/*",
    "laravel-vue-forms.d.ts",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/vue-fontawesome": "^3.0.0-3",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@types/lodash": "^4.14.168",
    "@types/luxon": "^1.26.5",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-typescript": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "@vue/compiler-sfc": "^3.0.5",
    "brace": "^0.11.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "lodash": "^4.17.21",
    "luxon": "^1.26.0",
    "minimist": "^1.2.5",
    "postcss": "^8.2.3",
    "rollup": "^2.36.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^3.8.3",
    "vue": "^3.0.5",
    "vue-class-store": "^3.0.0",
    "vue-multiselect": "^3.0.0-alpha.2",
    "vue-router": "^4.0.8",
    "vue-toastification": "^2.0.0-rc.1"
