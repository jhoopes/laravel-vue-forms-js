  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.15",
    "postcss-import": "^14.0.2",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.1.2",
    "vue-toastification": "^2.0.0-rc.1"
  }
}
